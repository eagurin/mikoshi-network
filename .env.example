# .env.example

# -----------------------------------
# Обязательные переменные среды
# -----------------------------------

# ---------------------------
# Конфигурация PostgreSQL
# ---------------------------
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DBNAME=postgres
POSTGRES_MAX_CONNECTIONS=1024

# ---------------------------
# Конфигурация Hatchet
# ---------------------------
HATCHET_POSTGRES_DBNAME=hatchet

# ---------------------------
# Настройки R2R сервиса
# ---------------------------
PORT=7272
HOST=0.0.0.0
CONFIG_PATH=./config/r2r.toml
R2R_PROJECT_NAME=r2r_default

# ---------------------------
# Конфигурация Hatchet Client
# ---------------------------
HATCHET_CLIENT_TLS_STRATEGY=none
HATCHET_CLIENT_GRPC_ADDRESS=hatchet-engine:7077
HATCHET_CLIENT_GRPC_MAX_RECV_MESSAGE_LENGTH=13421772800
HATCHET_CLIENT_GRPC_MAX_SEND_MESSAGE_LENGTH=13421772800


# -----------------------------------
# Опциональные переменные среды
# -----------------------------------

# ---------------------------
# Настройки Proxy (если необходимо)
# ---------------------------
# HTTP_PROXY=http://your-proxy.com:8080
# HTTPS_PROXY=http://your-proxy.com:8443
# NO_PROXY=localhost,127.0.0.1,.yourdomain.com

# ---------------------------
# API ключи и внешние сервисы (если используются)
# ---------------------------
# OPENAI_API_KEY=your-openai-api-key
# OPENAI_API_BASE=https://api.openai.com/v1

# ANTHROPIC_API_KEY=your-anthropic-api-key

# AZURE_API_KEY=your-azure-api-key
# AZURE_API_BASE=https://azure.microsoft.com/
# AZURE_API_VERSION=2021-01-01

# GOOGLE_APPLICATION_CREDENTIALS=/path/to/credentials.json

# VERTEX_PROJECT=your-vertex-project
# VERTEX_LOCATION=your-vertex-location

# AWS_ACCESS_KEY_ID=your-aws-access-key-id
# AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key
# AWS_REGION_NAME=your-aws-region

# GROQ_API_KEY=your-groq-api-key
# COHERE_API_KEY=your-cohere-api-key
# ANYSCALE_API_KEY=your-anyscale-api-key

# ---------------------------
# Конфигурация Ollama (если используется)
# ---------------------------
# OLLAMA_API_BASE=http://ollama:11434

# ---------------------------
# Конфигурация Unstructured API (если используется)
# ---------------------------
# UNSTRUCTURED_API_KEY=your-unstructured-api-key
# UNSTRUCTURED_API_URL=https://api.unstructured.io/general/v0/general
# UNSTRUCTURED_LOCAL_URL=http://unstructured:7275
# UNSTRUCTURED_NUM_WORKERS=10

# ---------------------------
# Дополнительные настройки
